class Solution {
    int lowerBound(int[] arr, int target) {
        int l = 0;
        int h = arr.length-1;
        int ans = arr.length;
        while(l <= h ){
            int mid = l +( h-l)/2;
            if(arr[mid ] < target) l = mid + 1;
            else{ ans = mid;
            h = mid-1;
            }
            
        }
        return ans;
    }
}
  
